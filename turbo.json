{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "mentis#build": {
      "outputs": ["dist/**"]
    },
    "docs#build": {
      "outputs": [".next/**", "!.next/cache/**"]
    }
  }
}
